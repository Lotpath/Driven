<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <system.data>
    <DbProviderFactories>
      <add name="PostgreSQL Data Provider" invariant="Npgsql" description=".Net Framework Data Provider for PostgreSQL" type="Npgsql.NpgsqlFactory, Npgsql"/>
    </DbProviderFactories>
  </system.data>
  <connectionStrings>
    <clear />
    <add name="Master" connectionString="Server=localhost;Port=5432;Database=postgres;Userid=postgres;Password=postgres;" />
    <add name="Store" connectionString="Server=localhost;Port=5432;Database=driven;Userid=postgres;Password=postgres;Pooling=true;MinPoolSize=1;MaxPoolSize=20;ConnectionLifeTime=15" />
  </connectionStrings>
</configuration>